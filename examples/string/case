#!/bin/bash
# -*- coding: utf-8 -*-

# shellcheck source=/dev/null
# shellcheck disable=SC2154
if [[ -z "${BashLib}" ]]; then
    echo 'BashLib environment variable is not set' >&2
    exit 1
fi
# shellcheck source=/dev/null
# shellcheck disable=SC2154
source "${BashLib}/string/case"
source "${BashLib}/runner"


runner ucase 'bash'
runner ucase 'Bash'
runner ucase 'BASH'

runner lcase 'bash'
runner lcase 'Bash'
runner lcase 'BASH'

